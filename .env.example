# Database
DATABASE_URL="file:./database.db"

# User agent for API requests
USER_AGENT="surge: item-price-analysis-bot"

# OSRS RSS feed URL
RSS_PAGE_URL="https://secure.runescape.com/m=news/a=13/latest_news.rss?oldschool=true"

# OSRS Wiki API endpoints
MAPPING_API_URL="https://prices.runescape.wiki/api/v1/osrs/mapping"
LATEST_API_URL="https://prices.runescape.wiki/api/v1/osrs/latest"

# OpenAI configuration
OPENAI_API_KEY="sk-your-openai-api-key-here"
# Admin key (optional) - required for benchmark usage tracking
OPENAI_ADMIN_KEY="sk-admin-your-admin-key-here"
#
# Model options (benchmark results with voting=2):
#   BEST:     gpt-5-mini + medium = 100% F1, 100% precision, 61K tokens
#   FAST:     gpt-5-mini + low    = 90% F1, 100% precision, 68K tokens
#   BUDGET:   o4-mini + medium    = 87% F1, 100% precision, 68K tokens
#
# Extraction uses 2-run voting for consensus (configurable in hybridExtractor.js)
#
OPENAI_MODEL="gpt-5-mini"
# Reasoning effort for o4-mini/gpt-5-mini: low, medium, high
OPENAI_REASONING_EFFORT="medium"

# Telegram bot configuration
TELEGRAM_BOT_TOKEN="your-telegram-bot-token-here"
TELEGRAM_CHAT_ID="your-telegram-chat-id-here"

# Analysis configuration
# JSON array of price change types to send alerts for
INCLUDED_CHANGE_TYPES=["Price increase","Price decrease"]

# Minimum margin threshold for item significance (in GP)
MARGIN_THRESHOLD="1000000"

# Minimum price variance percentage (0.05 = 5%)
PRICE_VARIANCE_PERCENT="0.05"

# Rate limiting - seconds between RSS/page fetches
RATE_LIMIT_SECONDS="60"

# Data sync interval in minutes
DATA_SYNC_INTERVAL_MINUTES="360"

# Logging level (debug, info, warn, error)
LOG_LEVEL="info"
